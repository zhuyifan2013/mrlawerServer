<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>rongyun</servlet-name>
        <servlet-class>TestRongyun</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rongyun</servlet-name>
        <url-pattern>/rongyun</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>sendQuestion</servlet-name>
        <servlet-class>SendQuestion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sendQuestion</servlet-name>
        <url-pattern>/sendQuestion</url-pattern>
    </servlet-mapping>


</web-app>